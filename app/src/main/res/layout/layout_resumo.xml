<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/resumo_dia_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/resumo_hora_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/resumo_dia_id"
        app:layout_constraintStart_toEndOf="@+id/resumo_dia_id"
        app:layout_constraintTop_toTopOf="@+id/resumo_dia_id" />

    <TextView
        android:id="@+id/resumo_tipo_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/resumo_hora_id"
        app:layout_constraintStart_toEndOf="@+id/resumo_hora_id"
        app:layout_constraintTop_toTopOf="@+id/resumo_hora_id" />
</androidx.constraintlayout.widget.ConstraintLayout>